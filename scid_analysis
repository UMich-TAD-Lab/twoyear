notmom_now_count <- data.frame(agora4_now_count_notmom, agora5_now_count_notmom, agnos_now_count_notmom, gad_now_count_notmom, ganos_now_count_notmom, manic_now_count_notmom, hyp_now_count_notmom, manic_hyp_now_count_notmom, mannos_now_count_notmom, mdd_now_count_notmom, mdnos_now_count_notmom, ocd4_now_count_notmom, ocd5_now_count_notmom, ocnos_now_count_notmom, panic_now_count_notmom, panicd_now_count_notmom, pannos_now_count_notmom, pandnos_now_count_notmom, dys_now_count_notmom, dysnos_now_count_notmom, ptsd4_now_count_notmom, ptsd5_now_count_notmom, ptnos_now_count_notmom, socialp4_now_count_notmom, socialp5_now_count_notmom, socnos_now_count_notmom, phobia4_now_count_notmom, phobia5_now_count_notmom, phonos_now_count_notmom, sub1_dsm4_now_count_notmom, sub1_dsm5_now_count_notmom, sub1_nos_now_count_notmom, sub2_dsm4_now_count_notmom, sub2_dsm5_now_count_notmom, sub2_nos_now_count_notmom, sub3_dsm4_now_count_notmom, sub3_dsm5_now_count_notmom, sub3_nos_now_count_notmom)
notmom_now_count <- t(notmom_now_count)
