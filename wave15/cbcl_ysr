Criteria <- c("Normal", "Borderline", "Clinical")
Depression <- c(193, 2, 27)
Anxiety <- c(189, 2, 34)
df <- data.frame(Criteria, Depression, Anxiety)
rm(Criteria)
rm(Depression)
